{
  "extends": "next/core-web-vitals",
  "plugins": ["@typescript-eslint", "drizzle"],
  "rules": {
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": ["error", {}],
    "drizzle/enforce-delete-with-where": [
      "error",
      { "drizzleObjectName": ["db"] }
    ],
    "drizzle/enforce-update-with-where": [
      "error",
      { "drizzleObjectName": ["db"] }
    ]
  }
}
